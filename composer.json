{
    "name": "indatus/dispatcher",
    "type": "laravel-library",
    "description": "Schedule your artisan commands within your application",
    "keywords": ["laravel", "artisan", "dispatcher", "schedule"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Kuhl",
            "email": "bkuhl@indatus.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "wp-cli/php-cli-tools": "v0.9.4-patch46",
        "mtdowling/cron-expression" : "~1.0.3",
        "composer/installers": "^1.9"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Indatus\\Dispatcher\\": "src/Indatus/Dispatcher"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Indatus\\Dispatcher\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
